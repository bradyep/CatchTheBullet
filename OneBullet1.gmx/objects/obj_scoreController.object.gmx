            <kind>1</kind>
            <string>// Check to see if there are any enemies left
if (instance_number(obj_zombie) &lt; 1)
{
    // Remove Player, PlayerArm and Bullet
    with (obj_bullet) if instance_number(object_index) &gt; 0 instance_destroy();
    with (obj_playerArm) if instance_number(object_index) &gt; 0 instance_destroy();
    with (obj_player) if instance_number(object_index) &gt; 0 instance_destroy();
    
    // Place the player in a random location
    instance_create(random(room_width),random(room_height),obj_player);
    instance_create(obj_player.x,obj_player.y,obj_playerArm);
    
    // Put stageNumber + 1 number of enemies back onto the room
    for (i=0; i &lt; stageNumber + 1; i++)
    {
        numberOfTries = 1;
        while (numberOfTries &lt; 100) {
            potentialEnemyX = random(room_width);
            potentialEnemyY = random(room_height);
            if ((abs(obj_player.x - potentialEnemyX) &gt; 50) &amp;&amp; (abs(obj_player.y - potentialEnemyY) &gt; 50))
            {
                numberOfTries = 200;
                instance_create(potentialEnemyX,potentialEnemyY,obj_zombie);
            }
            else
            {
                numberOfTries ++;
            }   
        } // End of while loop
    } // End of for loop

    // Increment Stage Number    
    stageNumber++;    
}

</string>
